<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= title %></title>
    <link rel="stylesheet" href="/css/style.css">
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css" rel="stylesheet">
	<script>
	  window.va = window.va || function () { (window.vaq = window.vaq || []).push(arguments); };
	</script>
	<script defer src="/_vercel/insights/script.js"></script>
</head>
<body>
    <!-- Navigation -->
    <nav class="navbar">
        <div class="nav-container">
            <ul class="nav-menu">
                <li class="nav-item">
                    <a href="#home" class="nav-link active">Home</a>
                </li>
                <li class="nav-item">
                    <a href="#skills" class="nav-link">Skills</a>
                </li>
                <li class="nav-item">
                    <a href="#experience" class="nav-link">Experience</a>
                </li>
                <li class="nav-item">
                    <a href="#projects" class="nav-link">Portfolio</a>
                </li>
                <li class="nav-item">
                    <a href="#documents" class="nav-link">Professional Documents</a>
                </li>
                <li class="nav-item">
                    <a href="#about" class="nav-link">About</a>
                </li>
                <li class="nav-item">
                    <a href="#contact" class="nav-link">Contact</a>
                </li>
            </ul>
            <div class="hamburger">
                <span class="bar"></span>
                <span class="bar"></span>
                <span class="bar"></span>
            </div>
        </div>
    </nav>

    <!-- Hero Section -->
    <section id="home" class="hero">
        <div class="hero-container">
            <div class="hero-content">
                <!-- Top Section: Name & Designation -->
                <div class="hero-top">
                    <h1 class="hero-title">Hi, I'm <span class="highlight"><%= name %></span></h1>
                    <h2 class="hero-subtitle"><%= tagline %></h2>
                </div>
                
                <!-- Bottom Section: Description & Buttons -->
                <div class="hero-bottom">
                    <p class="hero-description"><%= about %></p>
                    <div class="hero-buttons">
                        <a href="#projects" class="btn btn-primary">View My Work</a>
                        <a href="#contact" class="btn btn-secondary">Get In Touch</a>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <!-- Enhanced Skills Section -->
    <section class="skills" id="skills">
        <div class="container">
            <h2 class="section-title">Skills</h2>
            <div class="skills-grid">
                <!-- VECTOR -->
                <div class="skill-item" data-level="expert">
                    <img src="images/vector.png" alt="VECTOR" class="skill-icon">
                    <span class="skill-name">VECTOR Tools</span>
                    <div class="skill-description">
                        Expert in VECTOR CANoe/CANalyzer for automotive network analysis, diagnostics, and ECU testing with comprehensive protocol support.
                    </div>
                    <div class="skill-level">
                        <div class="proficiency-label">Expert</div>
                        <div class="progress-bar">
                            <div class="progress-fill"></div>
                        </div>
                    </div>
                </div>

                <!-- dSPACE -->
                <div class="skill-item" data-level="advanced">
                    <img src="images/dspace.png" alt="dSPACE" class="skill-icon">
                    <span class="skill-name">dSPACE Tools</span>
                    <div class="skill-description">
                        Advanced proficiency in dSPACE HIL systems for ECU testing, real-time simulation, and rapid prototyping in automotive development.
                    </div>
                    <div class="skill-level">
                        <div class="proficiency-label">Advanced</div>
                        <div class="progress-bar">
                            <div class="progress-fill"></div>
                        </div>
                    </div>
                </div>

                <!-- IPG CarMaker -->
                <div class="skill-item" data-level="advanced">
                    <img src="images/ipg-carmaker.png" alt="IPG CarMaker" class="skill-icon">
                    <span class="skill-name">IPG CarMaker</span>
                    <div class="skill-description">
                        Advanced skills in IPG CarMaker for vehicle dynamics simulation, ADAS testing, and virtual validation environments.
                    </div>
                    <div class="skill-level">
                        <div class="proficiency-label">Advanced</div>
                        <div class="progress-bar">
                            <div class="progress-fill"></div>
                        </div>
                    </div>
                </div>

                <!-- Robot Framework -->
                <div class="skill-item" data-level="basic">
                    <img src="images/robot-framework.png" alt="Robot Framework" class="skill-icon">
                    <span class="skill-name">Robot Framework</span>
                    <div class="skill-description">
                        Robot Framework basic skills in automated testing, test case development, and continuous integration in automotive projects.
                    </div>
                    <div class="skill-level">
                        <div class="proficiency-label">Basic</div>
                        <div class="progress-bar">
                            <div class="progress-fill"></div>
